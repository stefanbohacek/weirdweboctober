  {
    "lines": [
      "in a fleeting life",
      "some leisure time",
      "this autumn evening"
    ],
    "author": "Buson"
  },
  {
    "lines": [
      "the moon's guidance:",
      "please come this way",
      "a traveler's inn"
    ],
    "author": "Basho"
  },
  {
    "lines": [
      "one leaf falls",
      "alas! another",
      "with the wind"
    ],
    "author": "Ransetsu"
  },
  {
    "lines": [
      "cool summer breeze",
      "a thousand green mountains",
      "a lone temple"
    ],
    "author": "Shiki"
  },
  {
    "lines": [
      "on a withered branch",
      "sits a crow",
      "autumn nightfall"
    ],
    "author": "Basho"
  },
  {
    "lines": [
      "on this road",
      "goes no one",
      "autumn evening"
    ],
    "author": "Basho"
  },
  {
    "lines": [
      "The stars have already",
      "opened",
      "their autumn eyes"
    ],
    "author": "Ozaki Koyo"
  },
  {
    "lines": [
      "six in the morningâ€”",
      "my trip for moon-viewing",
      "ends at dawn"
    ],
    "author": "Basho"
  },
  {
    "lines": [
      "At the crescent moon",
      "the feeling",
      "of silence"
    ],
    "author": "Chiyo-ni"
  },
  {
    "lines": [
      "spring at my hut",
      "there is nothing...",
      "yet, there is everything"
    ],
    "author": "Sodo"
  }
